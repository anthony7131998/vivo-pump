            {
                "predicate": {
                    "single": true, 
                    "ref": "vivo:totalAwardAmount"
                }, 
                "object": {
                    "filter": "improve_dollar_amount",
                     "datatype": "xsd:decimal",
                    "literal": true
                }
            }
        ]
    }
