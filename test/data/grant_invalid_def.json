        "deptid": [
            {
                "predicate": {
                    "single": true, 
                    "ref": "vivo:administeredBy"
                }, 
                "object": {
                    "filter": "improve_deptid", 
                    "literal": true,
                    "datatype": "xsd:integer",
                    "enum": "data/deptid.txt"
                }
            }
        ], 
        "direct_costs": [
            {
                "predicate": {
                    "single": true, 
                    "ref": "vivo:grantDirectCosts"
                }, 
                "object": {
                    "filter": "improve_dollar_amount",
                    "datatype": "xsd:decimal",
                    "literal": true
                }
            }
        ], 
        "cois": [
            {
                "predicate": {
                    "single": false, 
                    "ref": "vivo:relatedRole"
                }, 
                "object": {
                    "literal": false, 
                    "type": "vivo:Co-principalInvestigatorRole",
                    "comment":  "the name value below is invalid as it matches one of the column names",
                    "name": "cois"
                }
            }, 
            {
                "predicate": {
                    "single": true, 
                    "ref": "vivo:co-PrincipalInvestigatorRoleOf"
                }, 
                "object": {
                    "literal": false,
                    "enum": "data/ufid.txt"
                }
            }
