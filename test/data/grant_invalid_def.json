            {
                "predicate": {
                    "single": true, 
                    "ref": "vivo:dateTime"
                }, 
                "object": {
                    "filter": "improve_date",
                    "datatype": "xsd:datetime",
                    "literal": true
                }
            }
        ], 
        "title": [
            {
                "predicate": {
                    "single": true, 
                    "ref": "rdfs:label"
