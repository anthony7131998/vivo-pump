{
    "entity_def": {
        "order_by": "title", 
        "entity_sparql": "?uri a vivo:Grant . ", 
        "type": "http://vivoweb.org/ontology/core#Grant"
    }, 
    "column_defs": {
        "deptid": [
            {
                "predicate": {
                    "single": true, 
                    "ref": "http://vivoweb.org/ontology/core#administeredBy"
                }, 
                "object": {
                    "filter": "improve_deptid", 
                    "literal": true, 
                    "enum": "deptid"
                }
            }
        ], 
        "direct_costs": [
            {
                "predicate": {
                    "single": true, 
                    "ref": "http://vivoweb.org/ontology/core#grantDirectCosts"
                }, 
                "object": {
                    "filter": "improve_dollar_amount", 
                    "literal": true
                }
            }
        ], 
        "title": [
            {
                "predicate": {
                    "single": true, 
                    "ref": "http://www.w3.org/2000/01/rdf-schema#label"
                }, 
                "object": {
                    "filter": "improve_title", 
                    "literal": true
                }
            }
        ], 
        "sponsor_award_id": [
            {
                "predicate": {
                    "single": true, 
                    "ref": "http://vivoweb.org/ontology/core#sponsorAwardId"
                }, 
                "object": {
                    "filter": "improve_sponsor_award_id", 
                    "literal": true
                }
            }
        ], 
        "sponsorid": [
            {
                "predicate": {
                    "single": true, 
                    "ref": "http://vivoweb.org/ontology/core#grantAwardedBy"
                }, 
                "object": {
                    "literal": true, 
                    "enum": "sponsorid"
                }
            }
        ], 
        "award_id": [
            {
                "predicate": {
                    "single": true, 
                    "ref": "http://vivoweb.org/ontology/core#localAwardId"
                }, 
                "object": {
                    "literal": true
                }
            }
        ], 
        "total_award": [
            {
                "predicate": {
                    "single": true, 
                    "ref": "http://vivoweb.org/ontology/core#totalAwardAmount"
                }, 
                "object": {
                    "filter": "improve_dollar_amount", 
                    "literal": true
                }
            }
        ]
    }
}