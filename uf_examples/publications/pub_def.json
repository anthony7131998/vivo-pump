{
    "entity_def": {
        "type": "bibo:AcademicArticle",
        "entity_sparql": "?uri vitro:mostSpecificType bibo:AcademicArticle .",
        "order_by": "title"
    }, 
    "column_defs": {
        "doi": [
            {
                "predicate": {
                    "single": true, 
                    "ref": "bibo:doi"
                }, 
                "object": {
                    "literal": true
                }
            }
        ], 
        "title": [
            {
                "predicate": {
                    "single": true, 
                    "ref": "rdfs:label"
                }, 
                "object": {
                    "filter": "improve_title", 
                    "literal": true
                }
            }
        ],
        "start_page": [
            {
                "predicate": {
                    "single": true,
                    "ref": "bibo:pageStart"
                },
                "object": {
                    "literal": true
                }
            }
        ],
        "pub_date":[
            {
                "predicate": {
                    "single": true,
                    "ref": "vivo:dateTimeValue"
                },
                "object": {
                    "literal": false,
                    "type": "vivo:dateTime",
                    "name": "pubDateTime"
                }
            },
            {
                "predicate": {
                    "single": true,
                    "ref": "vivo:dateTime"
                },
                "object": {
                    "literal": true
                }
            }
        ],
        "number":[
            {
                "predicate": {
                    "single": true,
                    "ref": "bibo:number"
                },
                "object": {
                    "literal": true
                }
            }
        ],
        "volume":[
            {
                "predicate": {
                    "single": true,
                    "ref":"bibo:volume"
                },
                "object": {
                    "literal": true
                }
            }
        ],
        "author": [
            {
                "predicate": {
                    "single": false,
                    "ref": "vivo:relates"
                },
                "object": {
                    "literal": false,
                    "type": "vivo:Authorship",
                    "name": "paperAuthroship"
                }
            },
            {
                "predicate": {
                    "single": true,
                    "ref": "vivo:Authorship"
                },
                "object": {
                    "literal": false,
                    "type": "vivo:relates",
                    "name": "relatingAuthor"
                },
                "predicate": {
                    "single": true,
                    "ref": "vivo:relates"
                },
                "object": {
                    "literal": false
                }
            }
        ]
    }
}